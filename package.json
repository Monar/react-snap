{
  "name": "@mastermonar/react-snap",
  "version": "1.25.0",
  "description": "Zero-configuration framework-agnostic static prerendering for SPAs",
  "main": "index.js",
  "author": "Monar",
  "license": "MIT",
  "repository": "monar/react-snap",
  "engines": {
    "node": ">= 10.18.1"
  },
  "dependencies": {
    "clean-css": "4.2.3",
    "express": "4.17.1",
    "express-history-api-fallback": "2.2.1",
    "glob-to-regexp": "0.4.1",
    "highland": "2.13.5",
    "html-minifier": "4.0.0",
    "minimalcss": "0.9.0",
    "mkdirp": "1.0.4",
    "puppeteer": "3.0.0",
    "serve-static": "1.14.1",
    "sourcemapped-stacktrace-node": "2.1.8"
  },
  "devDependencies": {
    "dev-null-stream": "0.0.1",
    "jest": "25.4.0",
    "markdown-toc": "1.2.0",
    "prettier": "2.0.4"
  },
  "scripts": {
    "toc": "yarn run markdown-toc -i doc/recipes.md",
    "test": "jest",
    "precommit": "prettier --write {*,src/*}.{js,json,css}"
  },
  "bin": {
    "react-snap": "./run.js"
